<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>Login_Valid</servlet-name>
        <servlet-class>college_library.Login_Valid</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>college_library.Registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Fine</servlet-name>
        <servlet-class>college_library.Fine</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin_Valid</servlet-name>
        <servlet-class>college_library.Admin_Valid</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Lib_Register</servlet-name>
        <servlet-class>college_library.Lib_Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login_Valid</servlet-name>
        <url-pattern>/Login_Valid</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Fine</servlet-name>
        <url-pattern>/Fine</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin_Valid</servlet-name>
        <url-pattern>/Admin_Valid</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Lib_Register</servlet-name>
        <url-pattern>/Lib_Register</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
